<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawerlayout"
    android:layout_width="match_parent"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".mainactivity">
    <TextView
        android:id="@+id/textView6"
        android:layout_width="90dp"
        android:layout_height="104dp"
        android:layout_marginLeft="295dp"
        android:layout_marginTop="260dp"
        android:background="@drawable/halfcirle"
         />



    <TextView
        android:id="@+id/textView"
        android:layout_width="317dp"
        android:layout_height="match_parent"
        android:background="#0E1229"
        />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="88dp"
        android:layout_height="5dp"
        android:layout_marginLeft="234dp"
        android:layout_marginTop="280dp"
        android:background="#958F8F"
        />


    <TextView
        android:id="@+id/textView3"
        android:layout_width="113dp"
        android:layout_height="35dp"
        android:layout_marginLeft="232dp"
        android:layout_marginTop="292dp"
        android:background="@drawable/roundtv"
        />
    <TextView
        android:id="@+id/textView5"
        android:layout_width="112dp"
        android:layout_height="5dp"
        android:layout_marginLeft="236dp"
        android:layout_marginTop="335dp"
        android:background="#958F8F"
        />


    <TextView
        android:id="@+id/title"
        android:layout_width="113dp"
        android:layout_height="35dp"

        android:layout_marginLeft="232dp"
        android:layout_marginTop="292dp"
        android:background="@drawable/roundcorner"
        android:gravity="center|clip_vertical|center_horizontal"
        android:text=":D Day "
        android:textColor="#050505"
        app:layout_constraintBottom_toTopOf="@+id/floatingActionButton" />


    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="286dp"
        android:layout_marginRight="90dp"
        android:layout_marginTop="400dp"
        android:clickable="true"
        android:tint="#F2F4F4"

        app:srcCompat="@android:drawable/ic_input_add" />

</RelativeLayout>